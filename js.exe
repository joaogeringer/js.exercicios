<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script type="text/javascript">
        var numero1 = prompt("Insira o primeiro numero:");
        var numero2 = prompt("Insira o segundo número:");

        var resultadoDivisao = divida(numero1, numero2);

        alert("O resultado da divisão é:" + resultadoDivisao)

        function divida(numero1, numero2){
            var resultado = 0;

            resultado = numero1 / numero2;

            return resultado;
        }
    </script>
</body>
</html>
